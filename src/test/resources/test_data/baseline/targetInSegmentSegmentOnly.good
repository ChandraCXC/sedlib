<?xml version="1.0" encoding="UTF-8"?>
<VOTABLE version="1.1"
  xmlns="http://www.ivoa.net/xml/VOTable/v1.1"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://www.ivoa.net/xml/VOTable/v1.1"
  >
  <RESOURCE utype="Sed">
    <TABLE utype="Segment">
      <GROUP utype="Target">
        <PARAM arraysize="*" datatype="char" name="Name" utype="Target.Name" value="segmentTarget"/>
        <PARAM arraysize="*" datatype="char" name="Description" utype="Target.Description" value="segmentDescription"/>
        <PARAM arraysize="*" datatype="char" name="Class" utype="Target.Class" value="segmentTargetClass"/>
        <PARAM arraysize="*" datatype="char" name="SpectralClass" utype="Target.SpectralClass" value="segmentSpectralClass"/>
        <PARAM arraysize="2" datatype="double" name="Pos" utype="Target.Pos" value="5.6, 7.8"/>
        <PARAM datatype="double" name="Redshift" ucd="" unit="" utype="Target.Redshift" value="3.4"/>
        <PARAM datatype="double" name="VarAmpl" ucd="" unit="" utype="Target.VarAmpl" value="1.2"/>
      </GROUP>
      <DATA>
        <TABLEDATA>
        </TABLEDATA>
      </DATA>
    </TABLE>
  </RESOURCE>
</VOTABLE>
