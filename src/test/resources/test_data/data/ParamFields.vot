<?xml version="1.0" encoding="UTF-8"?>
<!--
 ! This originated from the VOTable example in the standards document found at
 !    http://www.ivoa.net/Documents/REC/DM/SpectrumDM-20071029.pdf
 ! The original version (cvs rev 1.1 ) was obtained from /data/jcm/src/linux/java/ntest/new.vot
 !-->
    
<VOTABLE version="1.1"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="xmlns:http://www.ivoa.net/xml/VOTable/VOTable-1.1.xsd"
      xmlns:spec="http://www.ivoa.net/xml/SpectrumModel/Spectrum-1.01.xsd"
      xmlns="http://www.ivoa.net/xml/VOTable/v1.1">
  <RESOURCE utype="'RESOURCE'" >
        
    <TABLE utype="'TABLE'" >
      <GROUP utype="spec:Data">
        <GROUP utype="spec:Data.FluxAxis">
          <FIELDref ref="F1"/>
          <GROUP utype="spec:Data.FluxAxis.Accuracy">
            <FIELDref ref="F3"/>
            <PARAM name="SysErr" utype="spec:Data.FluxAxis.Accuracy.SysError" unit="unitSys" ucd="ucdSys" datatype="double" arraysize="1" value="15000"/>

            <PARAM name="FStatErrHigh" utype="spec:Data.FluxAxis.Accuracy.StatErrHigh" unit="unitErrH" ucd="ucdErrH" datatype="float" value="14000"/>

            <PARAM name="FQuality" utype="spec:Data.FluxAxis.Quality" unit="unitQuality" ucd="ucdQuality" datatype="int" value="12000"/>

          </GROUP>
        </GROUP>

        <GROUP utype="spec:Data.BackgroundModel">
          <FIELDref ref="BG2"/>
        </GROUP>

        <GROUP utype="spec:Data.SpectralAxis">
          <FIELDref ref="SP1"/>
            <PARAM name="Resolution" utype="spec:Data.SpectralAxis.Resolution" ucd="spect.resolution;em.wave"    unit="Angstrom" datatype="float" value="22000"/>
              
          <GROUP utype="spec:Data.SpectralAxis.Accuracy">
            <FIELDref ref="SP3"/>
            <PARAM name="BinSize" utype="spec:Data.SpectralAxis.Accuracy.BinSize" ucd="ucdBinL"    unit="unitBinL" datatype="float" value="88.90"/>
          </GROUP>
          <!-- In this case Resolution is demoted from Field to Param since it is constant -->
        </GROUP>
            
      </GROUP>
          
      <FIELD name="Flux" ID="F1" utype="spec:Data.FluxAxis.Value" ucd="phot.flux.density;em.wavelength" arraysize="1" datatype="double" unit="erg cm**(-2) s**(-1) Angstrom**(-1)" >
            <DESCRIPTION>This is the flux value</DESCRIPTION>
      </FIELD>

      <FIELD name="ErrorLow" ID="F3" utype="spec:Data.FluxAxis.Accuracy.StatErrLow" datatype="double" unit="erg cm**(-2) s**(-1) Angstrom**(-1)"/>

      <FIELD name="Coord" ID="SP1" utype="spec:Data.SpectralAxis.Value" ucd="em.wavelength" datatype="double" unit="Angstrom"/>

      <FIELD name="BinLow" ID="SP3" utype="spec:Data.SpectralAxis.Accuracy.BinLow" ucd="stat.min;em.wavelength" datatype="double" unit="Angstrom"/>

      <FIELD name="BGQuality" ID="BG2" utype="spec:Data.BackgroundModel.Quality" unit="fortnight" datatype="int" arraysize="1" />

      <DATA>
        <TABLEDATA>
          <TR><TD>1110.5</TD><TD>9205.0</TD><TD>1620.62</TD><TD>9800.82</TD><TD>110</TD> </TR>
          <TR><TD>2110.5</TD><TD>9205.0</TD><TD>2620.62</TD><TD>9800.82</TD><TD>120</TD> </TR>
          <TR><TD>3110.5</TD><TD>9205.0</TD><TD>3620.62</TD><TD>9800.82</TD><TD>130</TD> </TR>
        </TABLEDATA>
      </DATA>
    </TABLE>
  </RESOURCE>
</VOTABLE>

